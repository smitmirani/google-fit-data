# Google Fit API Credentials
# Copy this file to secrets.yml and fill in your actual credentials
# 
# To get these credentials:
# 1. Go to https://console.developers.google.com/apis/credentials
# 2. Create OAuth Client ID credentials
# 3. Create an API Key (Server Key)
# 4. Set up your project and get the Project ID

client_id: 'YOUR_CLIENT_ID_HERE'
client_secret: 'YOUR_CLIENT_SECRET_HERE'
fitness_api_key: 'YOUR_FITNESS_API_KEY_HERE'
redirect_uri: 'http://localhost'  # or 'https://developers.google.com/oauthplayground'
project_id: 'YOUR_PROJECT_ID_HERE'

